{
  "name": "pwhaas",
  "version": "1.3.0",
  "description": "Hash passwords with the same powerful hardware used by attackers",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdconley/pwhaas-js.git"
  },
  "scripts": {
    "compile": "tslint --project tslint.json && tsc -d && tsc -p . && babel -d dist/src-babeled/ dist/src/ && babel -d dist/test-babeled/ dist/test/ && cp -rf dist/src-babeled/* dist/src/ && cp -rf dist/test-babeled/* dist/test/",
    "prepublish": "rm -rf dist && npm run compile",
    "pretest": "npm run compile && cp test/*.js dist/test",
    "test": "mocha dist/test/*.js"
  },
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "keywords": [
    "password",
    "hash",
    "hashing",
    "security",
    "argon2"
  ],
  "author": {
    "name": "JD Conley",
    "email": "jd.conley@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/jdconley/pwhaas-js/issues"
  },
  "engines": {
    "node": "^4.0.0"
  },
  "files": [
    "dist/src"
  ],
  "license": "MIT",
  "dependencies": {
    "argon2themax": "^1.3.0",
    "babel-runtime": "^6.11.6",
    "lodash": "^4.17.11",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.0.33",
    "@types/chai": "^3.4.34",
    "@types/debug": "0.0.29",
    "@types/lodash": "^4.14.118",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.41",
    "@types/request": "0.0.31",
    "@types/request-promise": "^3.0.32",
    "babel-cli": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "chai": "^3.5.0",
    "mocha": "^3.0.2",
    "tslint": "^3.15.1",
    "typescript": "^2.9.2"
  }
}
